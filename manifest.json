{
  "author": "andrewss",
  "manifest_version": 3,
  "name": "Brainly Mentor",
  "host_permissions": [
    "*://znanija.com/*",
    "*://brainly.com/*"
  ],
  "permissions": ["storage"],
  "icons": {
    "128": "icons/icon.png"
  },
  "action": {
    "default_icon": "icons/icon.png"
  },
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["styles/*"]
  }]
}